{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Uniform Residential Loan Application (Form 1003)",
  "description": "JSON Schema for Freddie Mac Form 65 / Fannie Mae Form 1003, Effective 1/2021",
  "type": "object",
  "required": ["lender_info", "section_1_borrower_info", "section_2_financial_assets_liabilities", "section_4_loan_property_info", "section_5_declarations", "section_6_acknowledgments", "section_8_demographic_info"],
  "properties": {
    "lender_info": {
      "type": "object",
      "description": "Lender identification information",
      "required": ["lender_loan_number"],
      "properties": {
        "lender_loan_number": {
          "type": "string",
          "description": "Lender Loan Number"
        },
        "universal_loan_identifier": {
          "type": "string",
          "description": "Universal Loan Identifier (ULI)"
        },
        "agency_case_number": {
          "type": "string",
          "description": "Agency Case Number"
        }
      }
    },
    "section_1_borrower_info": {
      "type": "object",
      "description": "Section 1: Borrower Information",
      "required": ["personal_info", "current_employment"],
      "properties": {
        "personal_info": {
          "type": "object",
          "description": "1a. Personal Information",
          "required": ["name", "ssn_or_itin", "date_of_birth", "citizenship", "type_of_credit", "marital_status", "contact_info", "current_address"],
          "properties": {
            "name": {
              "type": "object",
              "required": ["first", "last"],
              "properties": {
                "first": {"type": "string"},
                "middle": {"type": "string"},
                "last": {"type": "string"},
                "suffix": {"type": "string"}
              }
            },
            "alternate_names": {
              "type": "array",
              "description": "Any names by which you are known or under which credit was previously received",
              "items": {
                "type": "object",
                "properties": {
                  "first": {"type": "string"},
                  "middle": {"type": "string"},
                  "last": {"type": "string"},
                  "suffix": {"type": "string"}
                }
              }
            },
            "ssn_or_itin": {
              "type": "string",
              "description": "Social Security Number or Individual Taxpayer Identification Number",
              "pattern": "^[0-9]{3}-[0-9]{2}-[0-9]{4}$"
            },
            "date_of_birth": {
              "type": "string",
              "description": "Date of birth in mm/dd/yyyy format",
              "pattern": "^(0[1-9]|1[0-2])/(0[1-9]|[12][0-9]|3[01])/[0-9]{4}$"
            },
            "citizenship": {
              "type": "string",
              "enum": ["US Citizen", "Permanent Resident Alien", "Non-Permanent Resident Alien"]
            },
            "type_of_credit": {
              "type": "object",
              "required": ["credit_type"],
              "properties": {
                "credit_type": {
                  "type": "string",
                  "enum": ["individual", "joint"]
                },
                "total_number_of_borrowers": {
                  "type": "integer",
                  "minimum": 1,
                  "description": "Required if joint credit"
                },
                "borrower_initials": {
                  "type": "array",
                  "description": "Initials of each borrower applying for joint credit",
                  "items": {"type": "string"}
                },
                "other_borrower_names": {
                  "type": "array",
                  "description": "Names of other borrowers applying for this loan",
                  "items": {"type": "string"}
                }
              }
            },
            "marital_status": {
              "type": "string",
              "enum": ["Married", "Separated", "Unmarried"]
            },
            "dependents": {
              "type": "object",
              "description": "Dependents not listed by another borrower",
              "properties": {
                "number": {"type": "integer", "minimum": 0},
                "ages": {
                  "type": "array",
                  "items": {"type": "integer"}
                }
              }
            },
            "contact_info": {
              "type": "object",
              "required": ["email"],
              "properties": {
                "home_phone": {
                  "type": "string",
                  "pattern": "^\\([0-9]{3}\\) [0-9]{3}-[0-9]{4}$"
                },
                "cell_phone": {
                  "type": "string",
                  "pattern": "^\\([0-9]{3}\\) [0-9]{3}-[0-9]{4}$"
                },
                "work_phone": {
                  "type": "string",
                  "pattern": "^\\([0-9]{3}\\) [0-9]{3}-[0-9]{4}$"
                },
                "work_phone_ext": {"type": "string"},
                "email": {
                  "type": "string",
                  "format": "email"
                }
              }
            },
            "current_address": {
              "type": "object",
              "required": ["street", "city", "state", "zip", "years", "months", "housing"],
              "properties": {
                "street": {"type": "string"},
                "unit": {"type": "string"},
                "city": {"type": "string"},
                "state": {"type": "string", "maxLength": 2},
                "zip": {"type": "string", "pattern": "^[0-9]{5}(-[0-9]{4})?$"},
                "country": {"type": "string", "default": "USA"},
                "years": {"type": "integer", "minimum": 0},
                "months": {"type": "integer", "minimum": 0, "maximum": 11},
                "housing": {
                  "type": "object",
                  "required": ["type"],
                  "properties": {
                    "type": {
                      "type": "string",
                      "enum": ["No primary housing expense", "Own", "Rent"]
                    },
                    "monthly_payment": {
                      "type": "number",
                      "minimum": 0,
                      "description": "Required if type is Rent"
                    }
                  }
                }
              }
            },
            "former_address": {
              "type": "object",
              "description": "Required if at current address for less than 2 years",
              "properties": {
                "street": {"type": "string"},
                "unit": {"type": "string"},
                "city": {"type": "string"},
                "state": {"type": "string", "maxLength": 2},
                "zip": {"type": "string", "pattern": "^[0-9]{5}(-[0-9]{4})?$"},
                "country": {"type": "string"},
                "years": {"type": "integer", "minimum": 0},
                "months": {"type": "integer", "minimum": 0, "maximum": 11},
                "housing": {
                  "type": "object",
                  "properties": {
                    "type": {
                      "type": "string",
                      "enum": ["No primary housing expense", "Own", "Rent"]
                    },
                    "monthly_payment": {"type": "number", "minimum": 0}
                  }
                }
              }
            },
            "mailing_address": {
              "type": "object",
              "description": "Mailing address if different from current address",
              "properties": {
                "street": {"type": "string"},
                "unit": {"type": "string"},
                "city": {"type": "string"},
                "state": {"type": "string", "maxLength": 2},
                "zip": {"type": "string", "pattern": "^[0-9]{5}(-[0-9]{4})?$"},
                "country": {"type": "string"}
              }
            }
          }
        },
        "current_employment": {
          "type": "object",
          "description": "1b. Current Employment/Self-Employment and Income",
          "required": ["employer_name", "phone", "address", "position", "start_date", "years_in_field", "months_in_field", "gross_monthly_income"],
          "properties": {
            "employer_name": {"type": "string"},
            "phone": {"type": "string"},
            "address": {
              "type": "object",
              "required": ["street", "city", "state", "zip"],
              "properties": {
                "street": {"type": "string"},
                "unit": {"type": "string"},
                "city": {"type": "string"},
                "state": {"type": "string"},
                "zip": {"type": "string"},
                "country": {"type": "string"}
              }
            },
            "position": {"type": "string"},
            "start_date": {
              "type": "string",
              "pattern": "^(0[1-9]|1[0-2])/(0[1-9]|[12][0-9]|3[01])/[0-9]{4}$"
            },
            "years_in_field": {"type": "integer", "minimum": 0},
            "months_in_field": {"type": "integer", "minimum": 0, "maximum": 11},
            "employed_by_party_to_transaction": {
              "type": "boolean",
              "description": "Employed by family member, property seller, real estate agent, or other party"
            },
            "business_owner_self_employed": {
              "type": "object",
              "properties": {
                "is_business_owner": {"type": "boolean"},
                "ownership_share": {
                  "type": "string",
                  "enum": ["less than 25%", "25% or more"]
                }
              }
            },
            "monthly_income_loss": {"type": "number"},
            "gross_monthly_income": {
              "type": "object",
              "properties": {
                "base": {"type": "number", "minimum": 0},
                "overtime": {"type": "number", "minimum": 0},
                "bonus": {"type": "number", "minimum": 0},
                "commission": {"type": "number", "minimum": 0},
                "military_entitlements": {"type": "number", "minimum": 0},
                "other": {"type": "number", "minimum": 0},
                "total": {"type": "number", "minimum": 0}
              }
            }
          }
        },
        "additional_employment": {
          "type": "object",
          "description": "1c. Additional Employment/Self-Employment and Income"
        },
        "previous_employment": {
          "type": "array",
          "description": "1d. Previous Employment/Self-Employment (at least 2 years history)",
          "items": {
            "type": "object",
            "properties": {
              "employer_name": {"type": "string"},
              "address": {
                "type": "object",
                "properties": {
                  "street": {"type": "string"},
                  "unit": {"type": "string"},
                  "city": {"type": "string"},
                  "state": {"type": "string"},
                  "zip": {"type": "string"},
                  "country": {"type": "string"}
                }
              },
              "position": {"type": "string"},
              "start_date": {"type": "string"},
              "end_date": {"type": "string"},
              "was_business_owner": {"type": "boolean"},
              "previous_gross_monthly_income": {"type": "number"}
            }
          }
        },
        "income_from_other_sources": {
          "type": "array",
          "description": "1e. Income from Other Sources",
          "items": {
            "type": "object",
            "required": ["income_source", "monthly_income"],
            "properties": {
              "income_source": {
                "type": "string",
                "enum": [
                  "Alimony",
                  "Automobile Allowance",
                  "Boarder Income",
                  "Capital Gains",
                  "Child Support",
                  "Disability",
                  "Foster Care",
                  "Housing or Parsonage",
                  "Interest and Dividends",
                  "Mortgage Credit Certificate",
                  "Mortgage Differential Payments",
                  "Notes Receivable",
                  "Public Assistance",
                  "Retirement",
                  "Royalty Payments",
                  "Separate Maintenance",
                  "Social Security",
                  "Trust",
                  "Unemployment Benefits",
                  "VA Compensation",
                  "Other"
                ]
              },
              "monthly_income": {"type": "number", "minimum": 0}
            }
          }
        }
      }
    },
    "section_2_financial_assets_liabilities": {
      "type": "object",
      "description": "Section 2: Financial Information - Assets and Liabilities",
      "properties": {
        "bank_accounts_retirement": {
          "type": "array",
          "description": "2a. Assets - Bank Accounts, Retirement, and Other Accounts",
          "items": {
            "type": "object",
            "required": ["account_type", "financial_institution", "cash_or_market_value"],
            "properties": {
              "account_type": {
                "type": "string",
                "enum": [
                  "Checking",
                  "Savings",
                  "Money Market",
                  "Certificate of Deposit",
                  "Mutual Fund",
                  "Stocks",
                  "Stock Options",
                  "Bonds",
                  "Retirement",
                  "Bridge Loan Proceeds",
                  "Individual Development Account",
                  "Trust Account",
                  "Cash Value of Life Insurance"
                ]
              },
              "financial_institution": {"type": "string"},
              "account_number": {"type": "string"},
              "cash_or_market_value": {"type": "number", "minimum": 0}
            }
          }
        },
        "other_assets_credits": {
          "type": "array",
          "description": "2b. Other Assets and Credits",
          "items": {
            "type": "object",
            "required": ["asset_or_credit_type", "cash_or_market_value"],
            "properties": {
              "asset_or_credit_type": {
                "type": "string",
                "enum": [
                  "Proceeds from Real Estate Property to be sold",
                  "Proceeds from Sale of Non-Real Estate Asset",
                  "Secured Borrowed Funds",
                  "Unsecured Borrowed Funds",
                  "Other",
                  "Earnest Money",
                  "Employer Assistance",
                  "Lot Equity",
                  "Relocation Funds",
                  "Rent Credit",
                  "Sweat Equity",
                  "Trade Equity"
                ]
              },
              "cash_or_market_value": {"type": "number", "minimum": 0}
            }
          }
        },
        "liabilities": {
          "type": "array",
          "description": "2c. Liabilities - Credit Cards, Other Debts, and Leases",
          "items": {
            "type": "object",
            "required": ["account_type", "company_name", "unpaid_balance", "monthly_payment"],
            "properties": {
              "account_type": {
                "type": "string",
                "enum": ["Revolving", "Installment", "Open 30-Day", "Lease", "Other"]
              },
              "company_name": {"type": "string"},
              "account_number": {"type": "string"},
              "unpaid_balance": {"type": "number", "minimum": 0},
              "to_be_paid_off_at_closing": {"type": "boolean"},
              "monthly_payment": {"type": "number", "minimum": 0}
            }
          }
        },
        "other_liabilities_expenses": {
          "type": "array",
          "description": "2d. Other Liabilities and Expenses",
          "items": {
            "type": "object",
            "required": ["liability_type", "monthly_payment"],
            "properties": {
              "liability_type": {
                "type": "string",
                "enum": ["Alimony", "Child Support", "Separate Maintenance", "Job Related Expenses", "Other"]
              },
              "monthly_payment": {"type": "number", "minimum": 0}
            }
          }
        }
      }
    },
    "section_3_real_estate": {
      "type": "object",
      "description": "Section 3: Financial Information - Real Estate",
      "properties": {
        "does_not_own_real_estate": {"type": "boolean"},
        "properties": {
          "type": "array",
          "description": "All properties currently owned",
          "items": {
            "type": "object",
            "required": ["address", "property_value", "status", "intended_occupancy"],
            "properties": {
              "address": {
                "type": "object",
                "required": ["street", "city", "state", "zip"],
                "properties": {
                  "street": {"type": "string"},
                  "unit": {"type": "string"},
                  "city": {"type": "string"},
                  "state": {"type": "string"},
                  "zip": {"type": "string"},
                  "country": {"type": "string"}
                }
              },
              "property_value": {"type": "number", "minimum": 0},
              "status": {
                "type": "string",
                "enum": ["Sold", "Pending Sale", "Retained"]
              },
              "intended_occupancy": {
                "type": "string",
                "enum": ["Investment", "Primary Residence", "Second Home", "Other"]
              },
              "monthly_insurance_taxes_dues": {
                "type": "number",
                "description": "If not included in monthly mortgage payment"
              },
              "monthly_rental_income": {
                "type": "number",
                "description": "For 2-4 unit primary or investment property"
              },
              "net_monthly_rental_income": {
                "type": "number",
                "description": "Calculated by lender"
              },
              "mortgage_loans": {
                "type": "array",
                "items": {
                  "type": "object",
                  "required": ["creditor_name", "monthly_payment", "unpaid_balance"],
                  "properties": {
                    "creditor_name": {"type": "string"},
                    "account_number": {"type": "string"},
                    "monthly_payment": {"type": "number", "minimum": 0},
                    "unpaid_balance": {"type": "number", "minimum": 0},
                    "to_be_paid_off_at_closing": {"type": "boolean"},
                    "loan_type": {
                      "type": "string",
                      "enum": ["FHA", "VA", "Conventional", "USDA-RD", "Other"]
                    },
                    "credit_limit": {"type": "number"}
                  }
                }
              }
            }
          }
        }
      }
    },
    "section_4_loan_property_info": {
      "type": "object",
      "description": "Section 4: Loan and Property Information",
      "required": ["loan_property_info"],
      "properties": {
        "loan_property_info": {
          "type": "object",
          "description": "4a. Loan and Property Information",
          "required": ["loan_amount", "loan_purpose", "property_address", "number_of_units", "property_value", "occupancy"],
          "properties": {
            "loan_amount": {"type": "number", "minimum": 0},
            "loan_purpose": {
              "type": "string",
              "enum": ["Purchase", "Refinance", "Other"]
            },
            "loan_purpose_other": {"type": "string"},
            "property_address": {
              "type": "object",
              "required": ["street", "city", "state", "county"],
              "properties": {
                "street": {"type": "string"},
                "unit": {"type": "string"},
                "city": {"type": "string"},
                "state": {"type": "string"},
                "county": {"type": "string"}
              }
            },
            "number_of_units": {"type": "integer", "minimum": 1},
            "property_value": {"type": "number", "minimum": 0},
            "occupancy": {
              "type": "string",
              "enum": ["Primary Residence", "Second Home", "Investment Property", "FHA Secondary Residence"]
            },
            "mixed_use_property": {
              "type": "boolean",
              "description": "Will you set aside space to operate your own business?"
            },
            "manufactured_home": {
              "type": "boolean",
              "description": "Is the property a manufactured home?"
            }
          }
        },
        "other_new_mortgage_loans": {
          "type": "array",
          "description": "4b. Other New Mortgage Loans on the Property",
          "items": {
            "type": "object",
            "required": ["creditor_name", "lien_type", "monthly_payment", "loan_amount"],
            "properties": {
              "creditor_name": {"type": "string"},
              "lien_type": {
                "type": "string",
                "enum": ["First Lien", "Subordinate Lien"]
              },
              "monthly_payment": {"type": "number", "minimum": 0},
              "loan_amount": {"type": "number", "minimum": 0},
              "credit_limit": {"type": "number"}
            }
          }
        },
        "rental_income_purchase": {
          "type": "object",
          "description": "4c. Rental Income on Property to Purchase (2-4 unit primary or investment)",
          "properties": {
            "expected_monthly_rental_income": {"type": "number"},
            "expected_net_monthly_rental_income": {"type": "number"}
          }
        },
        "gifts_or_grants": {
          "type": "array",
          "description": "4d. Gifts or Grants Received or to be Received",
          "items": {
            "type": "object",
            "required": ["asset_type", "deposited", "source", "cash_or_market_value"],
            "properties": {
              "asset_type": {
                "type": "string",
                "enum": ["Cash Gift", "Gift of Equity", "Grant"]
              },
              "deposited": {"type": "boolean"},
              "source": {
                "type": "string",
                "enum": [
                  "Community Nonprofit",
                  "Employer",
                  "Federal Agency",
                  "Local Agency",
                  "Relative",
                  "Religious Nonprofit",
                  "State Agency",
                  "Unmarried Partner",
                  "Lender",
                  "Other"
                ]
              },
              "cash_or_market_value": {"type": "number", "minimum": 0}
            }
          }
        }
      }
    },
    "section_5_declarations": {
      "type": "object",
      "description": "Section 5: Declarations",
      "required": ["property_and_money", "finances"],
      "properties": {
        "property_and_money": {
          "type": "object",
          "description": "5a. About this Property and Your Money for this Loan",
          "required": ["occupy_as_primary_residence"],
          "properties": {
            "occupy_as_primary_residence": {"type": "boolean"},
            "ownership_interest_last_3_years": {
              "type": "object",
              "properties": {
                "had_ownership": {"type": "boolean"},
                "property_type": {
                  "type": "string",
                  "enum": ["PR", "SR", "SH", "IP"],
                  "description": "Primary Residence, FHA Secondary Residence, Second Home, Investment Property"
                },
                "title_held": {
                  "type": "string",
                  "enum": ["S", "SP", "O"],
                  "description": "Self, Jointly with Spouse, Jointly with Other"
                }
              }
            },
            "family_or_business_affiliation_with_seller": {"type": "boolean"},
            "borrowing_undisclosed_money": {
              "type": "object",
              "properties": {
                "is_borrowing": {"type": "boolean"},
                "amount": {"type": "number"}
              }
            },
            "applying_for_other_mortgage": {"type": "boolean"},
            "applying_for_new_credit": {"type": "boolean"},
            "property_subject_to_lien": {"type": "boolean"}
          }
        },
        "finances": {
          "type": "object",
          "description": "5b. About Your Finances",
          "required": [
            "cosigner_or_guarantor",
            "outstanding_judgments",
            "delinquent_federal_debt",
            "lawsuit_party",
            "conveyed_title_in_lieu",
            "completed_preforeclosure_short_sale",
            "property_foreclosed",
            "declared_bankruptcy"
          ],
          "properties": {
            "cosigner_or_guarantor": {"type": "boolean"},
            "outstanding_judgments": {"type": "boolean"},
            "delinquent_federal_debt": {"type": "boolean"},
            "lawsuit_party": {"type": "boolean"},
            "conveyed_title_in_lieu": {"type": "boolean"},
            "completed_preforeclosure_short_sale": {"type": "boolean"},
            "property_foreclosed": {"type": "boolean"},
            "declared_bankruptcy": {
              "type": "object",
              "properties": {
                "has_declared": {"type": "boolean"},
                "bankruptcy_types": {
                  "type": "array",
                  "items": {
                    "type": "string",
                    "enum": ["Chapter 7", "Chapter 11", "Chapter 12", "Chapter 13"]
                  }
                }
              }
            }
          }
        }
      }
    },
    "section_6_acknowledgments": {
      "type": "object",
      "description": "Section 6: Acknowledgments and Agreements",
      "required": ["borrower_signature", "signature_date"],
      "properties": {
        "borrower_signature": {"type": "string"},
        "signature_date": {
          "type": "string",
          "pattern": "^(0[1-9]|1[0-2])/(0[1-9]|[12][0-9]|3[01])/[0-9]{4}$"
        },
        "additional_borrower_signature": {"type": "string"},
        "additional_borrower_signature_date": {"type": "string"}
      }
    },
    "section_7_military_service": {
      "type": "object",
      "description": "Section 7: Military Service",
      "required": ["has_served_or_serving"],
      "properties": {
        "has_served_or_serving": {"type": "boolean"},
        "service_details": {
          "type": "object",
          "properties": {
            "currently_serving": {
              "type": "object",
              "properties": {
                "is_serving": {"type": "boolean"},
                "projected_expiration_date": {"type": "string"}
              }
            },
            "retired_discharged_separated": {"type": "boolean"},
            "reserve_or_national_guard_only": {"type": "boolean"},
            "surviving_spouse": {"type": "boolean"}
          }
        }
      }
    },
    "section_8_demographic_info": {
      "type": "object",
      "description": "Section 8: Demographic Information",
      "properties": {
        "do_not_wish_to_provide": {"type": "boolean"},
        "ethnicity": {
          "type": "object",
          "properties": {
            "do_not_wish_to_provide": {"type": "boolean"},
            "hispanic_or_latino": {
              "type": "array",
              "items": {
                "type": "string",
                "enum": ["Mexican", "Puerto Rican", "Cuban", "Other Hispanic or Latino"]
              }
            },
            "other_hispanic_origin": {"type": "string"},
            "not_hispanic_or_latino": {"type": "boolean"}
          }
        },
        "sex": {
          "type": "object",
          "properties": {
            "do_not_wish_to_provide": {"type": "boolean"},
            "sex": {
              "type": "string",
              "enum": ["Female", "Male"]
            }
          }
        },
        "race": {
          "type": "object",
